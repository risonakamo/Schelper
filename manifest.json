{
    "manifest_version":2,
    "name":"SC helper",
    "version":"1.6.0",
    "description":"helper for SC website",
    "homepage_url":"https://github.com/risonakamo/schelper",

    "permissions":["tabs","*://chan.sankakucomplex.com/post/*","<all_urls>"],

    "web_accessible_resources":[
        "fitwindowall.js"
    ],

    "browser_action":{
        "default_popup":"popup.html"
    },

    "icons":{
        "16":"icon/icon-16.png",
        "48":"icon/icon-48.png",
        "128":"icon/icon-128.png"
    }
}